{
  "presets": [
    [
    "next/babel",
    {
      "preset-react": {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    },
    "@babel/preset-typescript"
  ]],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      },
      "@emotion/babel-plugin"
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "ssr": false,
            "displayName": false
          },
          "@emotion/babel-plugin"
        ]
      ]
    }
  }
}
